{
    "name": "devolon/eazybreak",
    "description": "eazybreak package implements Easy break as a gateway of `devolon/payment` package",
    "keywords": ["laravel", "payment", "EazyBreak"],
    "require": {
        "php": "^8.0",
        "laravel/framework": "^8.12",
        "devolon/payment": "^1.1",
        "devolon/laravel-common": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1"
    },
    "require-dev": {
        "mockery/mockery": "^1.4.2",
        "orchestra/testbench-core": "^6.23",
        "phpunit/phpunit": "^9.3.3"
    },
    "autoload": {
        "psr-4": {
            "Devolon\\EazyBreak\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Devolon\\EazyBreak\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Devolon technical team",
            "homepage": "https://www.devolon.fi"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Devolon\\EazyBreak\\DevolonEazyBreakServiceProvider"
            ]
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "dev"
}
